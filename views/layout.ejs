<body>

<!-- SEARCH BAR -->
<div id="nav">
	<div id="logo-div">
		<p>#VIRAL</p>
	</div>
	<div id="twit_search">
		<form role="form" id="search-form">
			<div class="form-group" id="search-bar">
				<input type="text" name="search" class="form-control" id="search" value="" placeholder="Bananas" />
			</div>
			<button id="search-button">Gimme!</button>
		</form>
	</div>
	<div id="link-to">
		<% if(user != undefined) { %>
				<p>
					<%= user.fb.email %>
				</p>
				<a href="/logout">Logout</a>
			<% } else { %> 
				<a href="/auth/facebook"><i class="fa fa-facebook-official fa-3x"></i></a>
		<% } %>
	</div>			
</div>

	

<!-- RENDER FEEDS -->
<p id="test"></p>

</div>

